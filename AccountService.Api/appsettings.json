{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "PasswordPolicy": {
    "MinLength": 8,
    "MaxLength": 64,
    "RequireLowercase": true,
    "RequireUppercase": true,
    "RequireDigit": true,
    "RequireSpecialChar": false
  }
}
